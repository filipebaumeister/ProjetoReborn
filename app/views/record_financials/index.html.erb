<p id="notice"><%= notice %></p>
<%= stylesheet_link_tag  'scaffolds', media: 'all', 'data-turbolinks-track' => true %>
<h1>Listing Record Financials</h1>

<table>
  <thead>
    <tr>
      <th>Financial</th>
      <th>Record</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @record_financials.each do |record_financial| %>
      <tr>
        <td><%= record_financial.FINANCIAL %></td>
        <td><%= record_financial.RECORD %></td>
        <td><%= link_to 'Show', record_financial %></td>
        <td><%= link_to 'Edit', edit_record_financial_path(record_financial) %></td>
        <td><%= link_to 'Destroy', record_financial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record financial', new_record_financial_path %>
