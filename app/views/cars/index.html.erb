<html>
<head>
<%= stylesheet_link_tag  'scaffolds', media: 'all', 'data-turbolinks-track' => true %>  
</head>  
<p id="notice"><%= notice %></p>

<h1>Listando Carros</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Fabyear</th>
      <th>Modelyear</th>
      <th>Fuel</th>
      <th>Km</th>
      <th>New</th>
      <th>Colorin</th>
      <th>Plate</th>
      <th>Transmission</th>
      <th>Fipevalue</th>
      <th>Specs</th>
      <th>Filial</th>
      <th colspan="3">Ações</th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.NAME %></td>
        <td><%= car.STATUS %></td>
        <td><%= car.FABYEAR %></td>
        <td><%= car.MODELYEAR %></td>
        <td><%= car.FUEL %></td>
        <td><%= car.KM %></td>
        <td><%= car.NEW %></td>
        <td><%= car.COLORIN %></td>
        <td><%= car.PLATE %></td>
        <td><%= car.TRANSMISSION %></td>
        <td><%= car.FIPEVALUE %></td>
        <td><%= car.SPECS %></td>
        <td><%= car.FILIAL %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Car', new_car_path %>


</html>