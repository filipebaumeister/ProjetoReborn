<head>
    <%= stylesheet_link_tag  'users', media: 'all', 'data-turbolinks-track' => true %>
</head>

<div class="left"></div>
<div class="right">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="content">
      <h2>Fa√ßa Login</h2>
        <%= f.email_field :email, autofocus: true, :placeholder => "E-Mail"%>
        <%= f.password_field :password, autocomplete: "off", :placeholder => "Senha" %>
        <%= f.submit "Entrar", :class => 'LoginButton'%>
      </form>
    </div>
  <% end %>
</div>